{
  "name": "RubyGems",
  "version": "1.0",
  "manifest_version": 2,
  "description": "RubyGems Search",
  "icons": {
    "48": "images/icon.png"
  },
  "permissions": [
    "tabs",
    "https://rubygems.org/*"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "RubyGems",
    "default_popup": "popup.html"
  }
}
